	<section class="section main-section even fp-auto-height-responsive" id="contact-section" data-anchor="contact">
		<div class="bg-clipper">
			<h1 class="hero-title pt-5" data-animation="dropDownAnim">Contact</h1>

			<form class="col-md-6 col-sm-8 offset-sm-2" id="contact-form" method="POST" data-animation="formAnim" action="/api/contact/">
				<div class="row">
					<div class="col-md-12">
						<label class="control-label">Email</label>
						<input placeholder="johndoe@gmail.com" type="email" name="email" id="email" required />
					</div>
				</div>
				<div class="row mt-2">
					<div class="col-md-12">
						<label class="control-label">Object</label>
						<input placeholder="Hello..." type="text" name="title" id="title" required />
					</div>
					<div class="col-md-12 mt-2">
						<label class="control-label">Message</label>
						<textarea class="mb-0" id="content" rows="8" name="content" placeholder="Write your message here..." required></textarea>
					</div>
				</div>

				<div class="submit-btn-wrapper">
					<span>Send</span>
					<input type="submit" class="action-btn" value="Send" id="submit-contact" />
				</div>
				<input type="hidden" name="_csrf" value="<%= locals.csrfToken %>" />

				<div class="col-md-12 p-0 mt-3 contact-alert-wrapper">
					<div id="contact-alert"></div>
				</div>
			</form>

			<img src="/svg/undraw_messages.svg" class="contact-svg" alt="Contact Illustration" data-animation="contactSvgAnim" draggable="false" />
			<div class="contact-footer">
				<ul>
					<li><i class="fas fa-phone"></i> <span>+33 6 89 89 81 98</span></li>
					<li><span>ablin.dev@gmail.com </span><i class="fas fa-envelope"></i></li>
				</ul>
			</div>
		</div>
